(define (1+ x)
  (+ x 1))

(define (-1+ x)
  (- x 1))

(define (+_ a b)
  (if (= a 0)
      b
      (1+ (+_ (-1+ a) b))))

(define (+__ a b)
  (if (= a 0)
      b
      (+__ (-1+ a) (1+ b))))

